{
	"rules": {
		"indent": ["error", "tab"],
		"no-var": "error",
		"prefer-const": "error",
		"no-trailing-spaces": "error",
		"comma-dangle": ["error"],
		"eol-last": "error",
		"no-multiple-empty-lines": ["error", { "max": 1 }],
		"no-multi-spaces": "error",
		"space-infix-ops": "error"
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	}
}
